{
  "name": "mern-shopping-cart-app",
  "version": "1.0.0",
  "description": "MERN shopping cart app",
  "main": "server.js",
  "type": "module",
  "proxy": "http://127.0.0.1:5000/",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StoyanBochukov/MERN-E-Comm.git"
  },
  "author": "Stoyan Bochukov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/StoyanBochukov/MERN-E-Comm/issues"
  },
  "homepage": "https://github.com/StoyanBochukov/MERN-E-Comm#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "concurrently": "^7.3.0",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.5.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^2.0.19",
    "react-router-dom": "^6.3.0"
  }
